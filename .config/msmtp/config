account elliot@elliotwilks.site
host mail.elliotwilks.site
port 465
from elliot@elliotwilks.site
user elliot@elliotwilks.site
passwordeval "pass elliot@elliotwilks.site"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/elliot/.local/state/msmtp/msmtp.log
tls_starttls off
